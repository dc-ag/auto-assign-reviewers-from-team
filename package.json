{
  "name": "auto-assign-reviewers-from-team",
  "version": "1.2.2",
  "private": true,
  "description": "GitHub Action to assign reviewers to a PR from a team",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt lib/main.js -o dist",
    "all": "npm run build && npm run format && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dc-ag/auto-assign-reviewers-from-team.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "dc AG",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^5.1.1"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-github": "^4.10.2",
    "eslint-plugin-jest": "^27.9.0",
    "js-yaml": "^4.1.0",
    "nock": "^13.5.4",
    "prettier": "2.8.8",
    "typescript": "^5.4.3"
  }
}
