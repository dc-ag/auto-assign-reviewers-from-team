{
  "name": "auto-assign-reviewers-from-team",
  "version": "1.0.0",
  "private": true,
  "description": "GitHub Action to assign reviewers to a PR from a team",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt lib/main.js -o dist",
    "all": "npm run build && npm run format && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dc-ag/auto-assign-reviewers-from-team.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "dc AG",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^5.0.3"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.23.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^26.8.7",
    "js-yaml": "^4.1.0",
    "nock": "^13.2.9",
    "prettier": "2.7.1",
    "typescript": "^4.8.2"
  }
}
